library('private-pipeline-library')
library('jenkins-shared')

def settings =[
    deployBranch: 'main',
    performSonarAnalysis: false,
    onSuccess: { build, env ->
      notifyChat(env: env, currentBuild: build, room: 'nxrm-notifications')
    },
    onFailure: { build, env ->
      notifyChat(env: env, currentBuild: build, room: 'nxrm-notifications')
    }
]

mavenSnapshotPipeline(settings)
